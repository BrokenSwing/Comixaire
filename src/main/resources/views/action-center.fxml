            <children>
                <ImageView layoutX="68.0" layoutY="44.0" fx:id="newItemButton">
                    <image>
                        <Image url="@/assets/geometrical-shapes-set-primary.png"/>
                    </image>
                </ImageView>
                <ImageView fitHeight="18.0" fitWidth="18.0" layoutX="124.0" layoutY="45.0">
                    <image>
                        <Image url="@/assets/add.png"/>
                    </image>
                </ImageView>
                <Text fill="#c4c4c4" layoutX="51.0" layoutY="162.0" strokeType="OUTSIDE" strokeWidth="0.0"
